q_value_adaptive=function(p_seq, pi0, alpha, sig)
{
n <- length(p_seq)
res <- NULL
q_est=sum(qvalue(p_seq,fdr.level=alpha,pi0=pi0)$significant)
res$est <- q_est
if (q_est == 0) {
res$fdr <- 0
res$power <- 0
} else {
res$fdr <- sum(1 - sig[1:q_est]) / q_est
res$power <- sum(sig[1:q_est]) / sum(sig)
}
return(res)
}
